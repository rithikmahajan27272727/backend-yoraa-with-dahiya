{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PublicReadGetObject",
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::yoraa-contabo-s3-bucket/*"
      ]
    },
    {
      "Sid": "AllowPublicAccess",
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:GetObjectVersion"
      ],
      "Resource": [
        "arn:aws:s3:::yoraa-contabo-s3-bucket/*"
      ]
    }
  ]
}
